      "src/index.js"
    ]
  },
  "keywords": [
    "javascript",
    "priority queue",
    "queue",
    "heap",
    "heaps",
    "algorithms",
    "data structures"
  ],
  "author": "Kevin Lee",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-minify": "^0.5.1",
    "jest": "^26.0.1"
  }
}
