{
  "name": "vanilla-priority-queue",
  "version": "1.0.1",
  "repository": "https://github.com/leekevinyg/js-priority-queue",
  "description": "javascript implementation of a priority queue",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
